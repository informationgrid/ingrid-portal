<page>
  <defaults
     skin="orange"
     layout-decorator="ingrid"
     portlet-decorator="ingrid-teaser"
  />
  <title>Statistiken</title>
  <metadata name="title" xml:lang="en">Stats</metadata>

  <fragment id="admin-statistics" type="layout" name="jetspeed-layouts::IngridOneColumn">
    <fragment id="admin-statistics-0" type="portlet" name="ingrid-portal-apps::AdminStatisticsPortlet">
      <property layout="OneColumn" name="row" value="0" />
    </fragment>
  </fragment>

  <security-constraints>
    <security-constraints-ref>admin</security-constraints-ref>
    <security-constraints-ref>admin-portal</security-constraints-ref>
    <security-constraints-ref>admin-partner</security-constraints-ref>
    <security-constraints-ref>admin-provider</security-constraints-ref>
  </security-constraints>
</page>