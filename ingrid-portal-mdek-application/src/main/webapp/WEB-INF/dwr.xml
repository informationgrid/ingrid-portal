<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN"
    "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="EntryService">
			<param name="beanName" value="entryService" />
		</create>
		<create creator="spring" javascript="SNSService">
			<param name="beanName" value="snsService" />
		</create>
		<create creator="spring" javascript="CTService">
			<param name="beanName" value="ctService" />
		</create>
		<create creator="spring" javascript="VersionInformation">
			<param name="beanName" value="versionInformation" />
		</create>
		<create creator="spring" javascript="DownloadTest">
			<param name="beanName" value="downloadTest" />
		</create>

		<convert converter="exception" match="java.lang.Exception" />
 		<convert converter="bean" match="java.lang.StackTraceElement" />

		<convert converter="bean" match="de.ingrid.mdek.beans.VersionInformation"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.AddressSearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.CatalogBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.CommentBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.CSVSearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.DataFormatBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.DBContentBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.JobInfoBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.KeyValuePair"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.LinkDataBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.LocationBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.MdekAddressBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.MdekDataBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.MediaOptionBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.ObjectSearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.OperationBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.OperationParameterBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.ScaleBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.SearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.TimeReferenceBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.UrlBean"/>
		<convert converter="bean" match="de.ingrid.mdek.beans.VectorFormatDetailsBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.cts.Coordinate"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.cts.CTSResponse"/>
		<convert converter="enum" match="de.ingrid.mdek.dwr.cts.SpatialReferenceSystem"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.sns.SNSTopic"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.sns.SNSLocationTopic"/>
		<convert converter="enum" match="de.ingrid.mdek.dwr.sns.Type"/>
	</allow>
</dwr>