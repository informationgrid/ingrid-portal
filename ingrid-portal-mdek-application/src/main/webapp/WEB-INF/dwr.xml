<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 1.0//EN"
    "http://www.getahead.ltd.uk/dwr/dwr10.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="EntryService">
			<param name="beanName" value="entryService" />
		</create>
		<create creator="spring" javascript="SNSService">
			<param name="beanName" value="snsService" />
		</create>
		<create creator="spring" javascript="CTService">
			<param name="beanName" value="ctService" />
		</create>
		<create creator="spring" javascript="VersionInformation">
			<param name="beanName" value="versionInformation" />
		</create>

		<convert converter="exception" match="java.lang.Exception" />
		<convert converter="bean" match="de.ingrid.mdek.VersionInformation"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.AddressSearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.CatalogBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.CommentBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.DataFormatBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.DBContentBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.JobInfoBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.LinkDataBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.LocationBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.MdekAddressBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.MdekDataBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.MediaOptionBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.ObjectSearchResultBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.OperationBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.OperationParameterBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.ScaleBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.TimeReferenceBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.UrlBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.VectorFormatDetailsBean"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.sns.SNSTopic"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.sns.SNSLocationTopic"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.cts.Coordinate"/>
		<convert converter="bean" match="de.ingrid.mdek.dwr.cts.CTSResponse"/>
		<convert converter="enum" match="de.ingrid.mdek.dwr.sns.Type"/>
		<convert converter="enum" match="de.ingrid.mdek.dwr.cts.SpatialReferenceSystem"/>
	</allow>
</dwr>