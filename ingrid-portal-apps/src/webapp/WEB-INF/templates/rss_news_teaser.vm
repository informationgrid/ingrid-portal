#foreach($rssEntry in $rssEntries)
  <div class="resultset">
    <span class="result_title"><strong><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link" /></span><a href="$!rssEntry.link" target="_new" title="Link öffnet in neuem Fenster">$!rssEntry.title</a></strong><em class="date">, $date.format('medium_date',$rssEntry.publishedDate)</em></span>
    <p class="result_description">
      $!rssEntry.description
    </p>
    <p class="result_metadata">
      Rubrik: xx<br />
#if($rssEntry.author)
	  Anbieter: $rssEntry.author
#end	  
    </p>

  </div>
  <div class="line"><hr /></div>
#end	
  <br />

  <div class="ico_linkintern"><span style="font-size:75%"><a href="portal/rss-news.psml">weitere Meldungen</a></span></div>