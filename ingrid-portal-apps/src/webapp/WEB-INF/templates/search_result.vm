<!-- RESULTS LEFT -->
  <div id="results_left">
    <div class="section">  
      <div class="resultset_nav float_left">
#renderRankedResultCounter($rankedPageSelector)
      </div>
      <div class="resultset_nav">
#renderRankedPageNavigation($rankedPageSelector)
      </div>
      <br />

#if($rankedResultList)
      #foreach($rs in $rankedResultList.hits)
    	  #if($velocityCount > 1)
			<div class="line"><hr /></div>
		  #end		  <div class="resultset">
		  #if ($rs.title)
		    #if ($rs.type == "nutch")
			  <span class="result_title"><strong><a href="$!rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title) </a></strong></span>
			#elseif($rs.type == "dsc")
			  #if($rs.udk_class)
				#set($udkObjClassNameKey = "udk_obj_class_name_$rs.udk_class")
				#set($udkObjClassName = $MESSAGES.getString($udkObjClassNameKey))
				<span class="result_title"><span class="icon_udk"><img src="/ingrid-portal-apps/images/icn_udk_obj_${rs.udk_class}.gif" alt="${udkObjClassName}" title="${udkObjClassName}"></span><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid" target="_new" title="Link öffnet in neuem Fenster">#renderTitle($rs.title) </a></strong></span>
			  #else
				<span class="result_title"><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title) </a></strong></span>
			  #end
			#else
			  <span class="result_title"><strong>#renderTitle($rs.title) </strong></span>
			#end		
            <p class="result_description">
            $!rs.abstract 
            </p>
            <p class="result_metadata">
##		    #if ($rs.type == "nutch")
## NO Partner set in result detail !!! Only in catalogues ?
##              Anbieter: $!rs.partner<br />
##			#else
              Anbieter: $!rs.provider<br />
##			#end
              Quelle: $!rs.source<br />
              Ranking: $!rs.score<br />
            </p>
			#if($rs.wms_url)
			<p class="result_link">
				<img src="/ingrid-portal-apps/images/icn_linkintern.gif" alt="interner Link" /><a href="portal/main-maps.psml?wms_url=$rs.wms_url" target="_new" title="Link öffnet in neuem Fenster">Zeige Karte</a>
			</p>				
			#end
			#if($rs.url)
			<p class="result_link">
              <a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster">$!rs.url_str</a>
            </p>
			#end
		  #else
			<span class="result_title"><strong>Suchergebnis ist leer</strong></span>
			<p class="result_link"></p>
		  #end
          </div>
	  #end
#end  

      <div class="resultset_nav float_left">
#renderRankedResultCounter($rankedPageSelector)
      </div>

      <div class="resultset_nav">
#renderRankedPageNavigation($rankedPageSelector)
      </div>

    </div>
    <!-- // PORTLET EINFACHE SUCHE -->    
  </div>
  
<!-- RESULTS RIGHT -->
  <div id="results_right">
    <div class="portlet">
      <div class="resultset_nav float_left">
#renderUnrankedResultCounter($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigation($unrankedPageSelector)
      </div>
      <br />

#if($unrankedResultList)
      #foreach($rs in $unrankedResultList.hits)
    	  <div class="resultset_partner_group">
            <div class="ico_down_black"><span class="title">$rs.title#if($rs.no_of_hits) ($rs.no_of_hits Treffer)#end</span><br /></div>
            <p class="result_description">
              $rs.abstract
            </p>
            <p class="result_metadata">
              Anbieter: $rs.provider
            </p>
          </div>

          <div class="resultset">
            <span class="result_title"><strong><a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>Zur Trefferliste</a></strong></span>
          </div>
          <br />
<!--
		  <div class="resultset">
		  #if ($rs.title)
			<span class="result_title"><strong><a href="$!rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title) </a></strong></span>
		  #else
			<span class="result_title"><strong>Suchergebnis ist leer</strong></span>
		  #end
          </div>
		  -->		  
      #end
#end
      
      <div class="resultset_nav float_left">
#renderUnrankedResultCounter($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigation($unrankedPageSelector)
      </div>
    </div>    
  </div>

  <div class="clearer"></div>
