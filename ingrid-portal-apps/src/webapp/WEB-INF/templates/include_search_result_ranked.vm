	<div class="section">
		#parse ('/WEB-INF/templates/include_search_result_counter.vm')
      <div class="resultset_nav">
        &nbsp;
      </div>
	  #parse ('/WEB-INF/templates/include_search_result_navigation.vm')
      <br />
#if($rankedSearchFinished && $rankedResultList && "$rankedResultList.length()" != "0")
	#foreach($rs in $rankedResultList.hits)
        	#if($grouping)
				#parse ('/WEB-INF/templates/include_search_result_group_header.vm')
        	#else
        		#if($velocityCount > 1)
        			<div class="line"><hr /></div>
        		#end
        	#end
		  <div class="resultset">
		  #if ($rs.title)
		    #if ($rs.type == "www-style")
			  <span class="result_title"><strong><a href="$!rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title) </a></strong></span>
			#elseif($rs.type == "detail-style")
			  #if($rs.udk_class)
				#set($udkObjClassNameKey = "udk_obj_class_name_$rs.udk_class")
				#set($udkObjClassName = $MESSAGES.getString($udkObjClassNameKey))
				<span class="result_title"><span class="icon_udk"><img src="/ingrid-portal-apps/images/icn_udk_obj_${rs.udk_class}.gif" alt="${udkObjClassName}" title="${udkObjClassName}"></span><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid#if($rs.alt_document_id)&altdocid=$rs.alt_document_id#end" target="_new" title="Link öffnet in neuem Fenster">#renderTitle($rs.title) </a></strong></span>
			  #else
				<span class="result_title"><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid#if($rs.alt_document_id)&altdocid=$rs.alt_document_id#end" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title) </a></strong></span>
			  #end
			#else
			  <span class="result_title"><strong>#renderTitle($rs.title) </strong></span>
			#end		
            <p class="result_description">
            $!rs.abstract 
            </p>
            <p class="result_metadata">
			#if($grouping)
				#if($grouping == 'partner')
					Anbieter: $!rs.provider<br />
				#end
			#else
            Anbieter: $!rs.provider<br />
			#end
            Quelle: $!rs.source<br />
            </p>
			#if($rs.wms_url)
			<p class="result_link">
				<img src="/ingrid-portal-apps/images/icn_linkintern.gif" alt="interner Link" /><a href="portal/main-maps.psml?wms_url=$rs.wms_url" target="_new" title="Link öffnet in neuem Fenster">Zeige Karte</a>
			</p>				
			#end
			#if($rs.url)
			<p class="result_link">
              <a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster">$!rs.url_str</a>
            </p>
			#end
		  #else
			<span class="result_title"><strong>Suchergebnis ist leer</strong></span>
			<p class="result_link"></p>
		  #end
          </div>
		  #parse ('/WEB-INF/templates/include_search_result_group_footer.vm')
	#end
#elseif($rankedSearchFinished)
		<p><strong>Keine Suchergebnisse gefunden.</strong><br /><br /></p>
#else
		<p><strong>Die Ausgabe der Suchergebnisse wurde vorzeitig unterbrochen.</strong><br /><br /></p>
#end  

		#parse ('/WEB-INF/templates/include_search_result_counter.vm')
      <div class="resultset_nav">
        &nbsp;
      </div>
	  #parse ('/WEB-INF/templates/include_search_result_navigation.vm')

    </div>
