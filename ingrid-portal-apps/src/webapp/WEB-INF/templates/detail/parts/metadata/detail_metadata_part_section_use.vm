#if($hasDetail4)
<div class="section">
    <a class="anchor" id="${hashDetail4}"></a>
    #renderSectionTitle($MESSAGES.getString("search.detail.usage"))
    <div class="row columns">
    ## Nutzungsbedingungen (useConstraints)
        #renderTextListTable($MESSAGES.getString("object_access.license") $useConstraints)
    ## Zugriffsbeschränkungen (accessConstraints)
        #set($consideredValues = ["otherRestrictions"])
        #renderTextListTable($MESSAGES.getString("object_access.restriction_value") $TOOL.mergeList($TOOL.getListOfValuesFromXPath($xPathUseAccessConstraints, "./gmd:MD_RestrictionCode/@codeListValue", "6010", $consideredValues), $TOOL.getSiblingsValuesFromXPath($xPathUseAccessConstraints, "gmd:otherConstraints", false, "6010", $consideredValues)))
    ## Anwendungseinschränkungen (useLimitation)
        #renderTextListTable($MESSAGES.getString("object_access.terms_of_use") $useLimitations)
    ## Sicherheitsstufung
        #parse("/WEB-INF/templates/detail/parts/metadata/use/use_classification_code.vm")
    ## Software
        #parse("/WEB-INF/templates/detail/parts/metadata/use/use_software.vm")
    </div>
</div>
#end