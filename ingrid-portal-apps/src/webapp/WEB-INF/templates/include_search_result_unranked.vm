	<div class="portlet">
      <div class="resultset_nav float_left">
#renderUnrankedResultCounter($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigation($unrankedPageSelector)
      </div>
      <br />

#if($unrankedResultList && "$unrankedResultList.length()" != "0")
      #foreach($rs in $unrankedResultList.hits)
    	  <div class="resultset_partner_group">
            <div class="ico_down_black"><span class="title">$rs.dataSourceName#if($rs.no_of_hits) ($rs.no_of_hits Treffer)#end</span><br /></div>
            <p class="result_description">
              $rs.dataSourceDescription
            </p>
            <p class="result_metadata">
              Anbieter: $rs.organisation
            </p>
          </div>

          <div class="resultset">
            <span class="result_title"><strong><a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($rs.title)</a></strong></span>
          </div>
##      #foreach($sub_rs in $rs.get('subHits'))
##          <div class="resultset">
##            <span class="result_title"><strong><a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($sub_rs.title)</a></strong></span>
##          </div>
##      #end
#if($rs.get('moreHits'))
      <div class="line"><hr /></div>
      <div class="resultset_show_partner">
#set ($action = $renderResponse.createActionURL())
#set ($groupSubjectKey = $rs.plugId)
$action.setParameter('action', 'showAllFromIPlug')
$action.setParameter('subject', $groupSubjectKey)
$action.setParameter('grouping', 'plugid')
        <span class="result_title"><img src="/ingrid-portal-apps/images/icn_linkintern.gif" alt="interner Link" /><a href="$action">Zeige alle Ergebnisse für $rs.dataSourceName</a></span>
      </div>
#end
          <br />
      #end
#elseif($unrankedSearchFinished)
		<p><strong>Keine Suchergebnisse gefunden.</strong><br /><br /></p>
#else
		<p><strong>Die Ausgabe der Suchergebnisse wurde vorzeitig unterbrochen.</strong><br /><br /></p>
#end
      
      <div class="resultset_nav float_left">
#renderUnrankedResultCounter($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigation($unrankedPageSelector)
      </div>
    </div> 
