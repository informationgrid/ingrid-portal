	<div class="portlet">
      <div class="resultset_nav float_left">
#renderUnrankedResultCounterGrouping($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigationGrouping($unrankedPageSelector)
      </div>
      <br />

#if($unrankedSearchFinished && $unrankedResultList && "$unrankedResultList.length()" != "0")
      #foreach($rs in $unrankedResultList.hits)
    	  <div class="resultset_partner_group">
            <div class="ico_down_black"><span class="title">$rs.dataSourceName#if($rs.no_of_hits) ($rs.no_of_hits $MESSAGES.getString('common.result.hits'))#end</span><br /></div>
            <p class="result_description">
              $rs.dataSourceDescription
            </p>
            <p class="result_metadata">
              $MESSAGES.getString('common.result.provider'): $rs.organisation
            </p>
          </div>

          <div class="resultset">
		  #if($rs.url)
              <span class="result_title"><strong><a href="$rs.url" target="_new" title="$MESSAGES.getString('common.tooltip.link.extern')"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="$MESSAGES.getString('common.alt.icon.extern')"/></span>#renderTitle($rs.title)</a></strong></span>
		  #elseif($rs.type == "detail-style")
		    #if($rs.udk_class)
		      #set($udkObjClassNameKey = "udk_obj_class_name_$rs.udk_class")
		      #set($udkObjClassName = $MESSAGES.getString($udkObjClassNameKey))
			  <span class="result_title"><span class="icon_udk"><img src="/ingrid-portal-apps/images/icn_udk_obj_${rs.udk_class}.gif" alt="${udkObjClassName}" title="${udkObjClassName}"></span><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid#if($rs.alt_document_id)&altdocid=$rs.alt_document_id#end" target="_new" title="$MESSAGES.getString('common.tooltip.link.extern')">#renderTitle($rs.title) </a></strong></span>
		    #else
			  <span class="result_title"><strong><a href="portal/search-detail.psml?docid=$rs.docid&plugid=$rs.plugid#if($rs.alt_document_id)&altdocid=$rs.alt_document_id#end" target="_new" title="$MESSAGES.getString('common.tooltip.link.extern')"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="$MESSAGES.getString('common.alt.icon.extern')"/></span>#renderTitle($rs.title) </a></strong></span>
		    #end
          #else
			  <span class="result_title"><strong>#renderTitle($rs.title)</strong></span>
		  #end
          </div>
				
##      #foreach($sub_rs in $rs.get('subHits'))
##          <div class="resultset">
##            <span class="result_title"><strong><a href="$rs.url" target="_new" title="Link öffnet in neuem Fenster"><span class="icon"><img src="/ingrid-portal-apps/images/icn_linkextern.gif" alt="externer Link"/></span>#renderTitle($sub_rs.title)</a></strong></span>
##          </div>
##      #end
#if($rs.get('moreHits'))
      <div class="line"><hr /></div>
      <div class="resultset_show_partner">
#set ($action = $renderResponse.createActionURL())
#set ($groupSubjectKey = $rs.plugId)
$action.setParameter('action', 'showAllFromIPlug')
$action.setParameter('subject', $groupSubjectKey)
$action.setParameter('grouping', 'plugid')
        <span class="result_title"><img src="/ingrid-portal-apps/images/icn_linkintern.gif" alt="$MESSAGES.getString('common.alt.icon.intern')" /><a href="$action">$MESSAGES.getString('common.result.group.showAll') $rs.dataSourceName</a></span>
      </div>
#end
          <br />
      #end
#elseif($unrankedSearchFinished)
		<p><strong>$MESSAGES.getString('common.result.noResults')</strong><br /><br /></p>
#else
		<p><strong>$MESSAGES.getString('common.result.interrupted')</strong><br /><br /></p>
#end
      
      <div class="resultset_nav float_left">
#renderUnrankedResultCounterGrouping($unrankedPageSelector)
      </div>

      <div class="resultset_nav">
        &nbsp;
      </div>
      <div class="resultset_nav">
#renderUnrankedPageNavigationGrouping($unrankedPageSelector)
      </div>
    </div> 
