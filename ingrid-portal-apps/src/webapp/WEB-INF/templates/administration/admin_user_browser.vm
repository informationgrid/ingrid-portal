#set ($action = $renderResponse.createActionURL())
<form action="$action" method="post" name="frmUserBrowser" id="frmUserBrowser">
	<div class="formbody formbody_border_style">
	<label for="filterCriteriaId">Login-Id</label>
	<input type="text" name="filterCriteriaId" id="filterCriteriaId" value="$!browser.filterCriteria.filterCriteriaId" />
    <label for="filterCriteriaFirstName">First Name</label>
    <input type="text" name="filterCriteriaFirstName" id="filterCriteriaFirstName" value="$!browser.filterCriteria.filterCriteriaFirstName" />
    <label for="filterCriteriaLastName">Last Name</label>
    <input type="text" name="filterCriteriaLastName" id="filterCriteriaLastName" value="$!browser.filterCriteria.filterCriteriaLastName" />
    <label for="filterCriteriaEmail">Email</label>
    <input type="text" name="filterCriteriaEmail" id="filterCriteriaEmail" value="$!browser.filterCriteria.filterCriteriaEmail" />
    <label for="filterCriteriaRole">Rolle</label>
    <input type="text" name="filterCriteriaRole" id="filterCriteriaRole" value="$!browser.filterCriteria.filterCriteriaRole" />
    #renderInputButton("doFilter" "" "doFilter" "Filter")
	<p>$MESSAGES.getString('account.view.tip')</p>
#renderDBBrowserNavigation($browser)
	<p>
		<table class="border fullwidth">
#set ($colNames = ["id", "firstName", "lastName", "email", "roles", "lastLogin"])
#set ($colTitles = [$MESSAGES.getString('account.view.id'), $MESSAGES.getString('account.view.first.name'), $MESSAGES.getString('account.view.last.name'), $MESSAGES.getString('account.view.email'), $MESSAGES.getString('account.view.roles'), $MESSAGES.getString('account.view.last.login')])
#renderDBBrowserHeader($colNames $colTitles)
#foreach ($dbEntity in $dbEntities)
	#set ($rowId = $dbEntity.id)
	#set ($rowValues = [$UtilsString.cutString($dbEntity.firstName, 20, 20), $UtilsString.cutString($dbEntity.lastName, 20, 20), $UtilsString.cutString($dbEntity.email, 40, 40), $dbEntity.roles, $dbEntity.lastLogin])
	#renderDBBrowserRowWithEditLink($rowId $rowValues)
#end
		</table>
		</p>
	</div>
#set ($buttons = ["doDelete", "doRefresh", "doNew"])
#renderDBBrowserFooterButtons($buttons)
</form>
