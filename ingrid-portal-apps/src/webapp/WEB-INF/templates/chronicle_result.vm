	<div class="section ">    
      <div class="resultset_nav float_left">
#renderResultCounterCatalogue($rankedPageSelector 'Umweltchronik')
      </div>
      <div class="resultset_nav">
#renderRankedPageNavigation($rankedPageSelector)
      </div>
      <br />
#foreach($rs in $rankedResultList.hits)
	#if($velocityCount > 1)
      <div class="line"><hr /></div>
	#end
		  <div class="resultset">
		  #if ($rs.title)
			<span class="result_title"><strong>#if ($rs.title == "")Kein Titel#else$rs.title#end</strong></span><span class="inquiry"><img src="/ingrid-portal-apps/images/icn_magnifier.gif" alt="Icon Lupe" /> <a href="$!rs.searchURL">Recherche</a></span>
            <p class="result_metadata">
              Ereignistyp: $!rs.type<br />
			  Datum: $!rs.date<br />
            </p>
			<p class="event_description">
			$!rs.abstract
			</p>
			<p class="result_link">
				<a href="$!rs.url" target="_new" title="Link öffnet in neuem Fenster">$!rs.url_str</a>
			</p>        
		  #else
			<span class="result_title"><strong>Ereignis ist leer</strong></span>
			<p class="result_link"></p>
		  #end
          </div>
#end
      <div class="resultset_nav float_left">
#renderResultCounterCatalogue($rankedPageSelector 'Umweltchronik')
      </div>
      <div class="resultset_nav">
#renderRankedPageNavigation($rankedPageSelector)
      </div>
    </div>
