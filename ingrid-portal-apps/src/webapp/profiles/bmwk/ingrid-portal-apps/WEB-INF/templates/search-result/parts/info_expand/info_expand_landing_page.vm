## Webseite
#if($landing_page && $landing_page.size() > 0)
    <div class="sub-section">
        #renderFactTitle("Webseite")
        <div class="document-list">
        #foreach($value in $landing_page)
            <div class="list-item">
                <div>
                    <a href="$value" class="icon external-link" title="$value" target="_blank" rel="noopener noreferrer">
                        <span class="ic-ic-external"></span>
                        <span class="text">
                            $value
                        </span>
                    </a>
                </div>
            </div>
        #end
        </div>
    </div>
#end