                #end
                
                #if($rs.additional_html_1 && !$rs.additional_html_1.isEmpty())
                    #set($hasPreview = true)
                #end
                
                #if($hasArea && $hasPreview)
                    #set($hasSwipe = true)
                #elseif($hasPreview && $!rs.additional_html_1.indexOf(">, <") > 0)
