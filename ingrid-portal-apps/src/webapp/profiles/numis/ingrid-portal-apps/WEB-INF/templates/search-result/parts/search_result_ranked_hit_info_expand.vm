                #set($swipeSlides = [])
                #set($hasArea = false)
                #set($hasPreview = false)
                #set($hasSwipe = false)
                
                #if($x1.isEmpty() == false && $y1.isEmpty() == false  && $x2.isEmpty() == false  && $y2.isEmpty() == false )
