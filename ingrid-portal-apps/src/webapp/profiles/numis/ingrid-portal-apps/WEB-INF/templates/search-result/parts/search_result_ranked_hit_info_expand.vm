                #end
                
                #if($hasSwipe)
                    <div class="swiper-container_${moreId} swiper-container">
                        <div class="swiper-wrapper">
                #end
                #if($hasPreview)
                    #if($!rs.additional_html_1.indexOf(">, <") == -1)
