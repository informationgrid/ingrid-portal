<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  	<class name="de.ingrid.portal.om.IngridNewsletterData" table="ingrid_newsletter_data">
		<id name="id">
    		<generator class="native"/>
        </id>
    	<property name="firstName" type="java.lang.String" column="firstname"/>
    	<property name="lastName" type="java.lang.String" column="lastname"/>
    	<property name="emailAddress" type="java.lang.String" column="email"/>
    	<property name="dateCreated" type="timestamp" column="created"/>
  </class>
</hibernate-mapping>
