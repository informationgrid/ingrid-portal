<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.ingrid.portal.om.IngridFragmentPrefValue" table="fragment_pref_value">
		<id name="id" column="PREF_VALUE_ID">
			<generator class="native" />
		</id>
		<property name="prefId" 			column="PREF_ID"		type="java.lang.Long" />
		<property name="prefValueOrder" 	column="VALUE_ORDER"	type="java.lang.Long" />
		<property name="prefValue" 			column="VALUE"			type="java.lang.String" />
	</class>
</hibernate-mapping>