<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  	<class name="de.ingrid.portal.om.IngridPrincipalPreference" table="ingrid_principal_pref">
		<id name="id">
    		<generator class="native"/>
        </id>
    	<property name="principalName" type="java.lang.String" column="principal_name"/>
    	<property name="prefName" type="java.lang.String" column="pref_name"/>
    	<property name="prefValue" type="java.lang.String" column="pref_value"/>
    	<property name="modifiedDate" type="timestamp" column="modified_date"/>
  </class>
</hibernate-mapping>
