<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.ingrid.portal.om.IngridJetspeedPrefsPropertyValue" table="prefs_property_value">
		<id name="propertyValueId" column="PROPERTY_VALUE_ID">
			<generator class="native" />
		</id>

		<property name="nodeId"	column="NODE_ID"		type="java.lang.Long" />
		<property name="propertyName" 	 	column="PROPERTY_NAME"			type="java.lang.String" />
		<property name="propertyValue"	column="PROPERTY_VALUE"		type="java.lang.String" />
	</class>
</hibernate-mapping>