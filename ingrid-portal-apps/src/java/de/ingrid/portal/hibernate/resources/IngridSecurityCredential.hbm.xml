<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.ingrid.portal.om.IngridSecurityCredential" table="security_credential">
		<id name="securityCredentialId" column="CREDENTIAL_ID">
			<generator class="native" />
		</id>
		<property name="securityPrincipalId" 	column="PRINCIPAL_ID"		type="java.lang.String" />
		<property name="securityColumnValue"	column="COLUMN_VALUE"		type="java.lang.String" />
		<property name="securityClassName" 	 	column="CLASSNAME"			type="java.lang.String" />
		<property name="securityCreationDate"	column="CREATION_DATE"		type="timestamp" />
		<property name="securityModifiedDate"	column="MODIFIED_DATE"		type="timestamp" />
		<property name="securityPrevAuthDate"	column="PREV_AUTH_DATE"		type="timestamp" />
		<property name="securityLastAuthDate" 	column="LAST_AUTH_DATE"		type="timestamp" />
	</class>
</hibernate-mapping>