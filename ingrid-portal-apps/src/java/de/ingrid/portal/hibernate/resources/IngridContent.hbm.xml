<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  	<class name="de.ingrid.portal.om.IngridContent" table="ingrid_content">
		<id name="id">
    		<generator class="native"/>
        </id>
    	<property name="itemKey" type="java.lang.String"/>
    	<property name="itemLang" type="java.lang.String"/>
    	<property name="itemTitle" type="java.lang.String"/>
    	<property name="itemValue" type="java.lang.String"/>
    	<property name="itemChanged" type="java.sql.Date"/>
    	<property name="itemChangedBy" type="java.lang.String"/>
  </class>
</hibernate-mapping>
