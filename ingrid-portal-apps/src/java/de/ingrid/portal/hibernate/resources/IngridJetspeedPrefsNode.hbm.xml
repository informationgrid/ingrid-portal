<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="de.ingrid.portal.om.IngridJetspeedPrefsNode" table="prefs_node">
		<id name="nodeId" column="NODE_ID">
			<generator class="native" />
		</id>

		<property name="parentNodeId"	column="PARENT_NODE_ID"		type="java.lang.Long" />
		<property name="nodeName" 	 	column="NODE_NAME"			type="java.lang.String" />
		<property name="fullPath"	column="FULL_PATH"		type="java.lang.String" />
	</class>
</hibernate-mapping>