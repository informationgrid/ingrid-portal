<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  	<class name="de.ingrid.portal.om.IngridAnniversary" table="ingrid_anniversary">
		<id name="id">
    		<generator class="native"/>
        </id>
    	<property name="topicId" column="topic_id" type="java.lang.String"/>
    	<property name="topicName" column="topic_name" type="java.lang.String"/>
    	<property name="dateFrom" column="date_from" type="java.lang.String"/>
    	<property name="dateFromYear" column="date_from_year" type="java.lang.Integer"/>
    	<property name="dateFromMonth" column="date_from_month" type="java.lang.Integer"/>
    	<property name="dateFromDay" column="date_from_day" type="java.lang.Integer"/>
    	<property name="dateTo" column="date_to" type="java.lang.String"/>
    	<property name="dateToYear" column="date_to_year" type="java.lang.Integer"/>
    	<property name="dateToMonth" column="date_to_month" type="java.lang.Integer"/>
    	<property name="dateToDay" column="date_to_day" type="java.lang.Integer"/>
    	<property name="administrativeId" column="administrative_id" type="java.lang.String"/>
    	<property name="fetched" type="java.sql.Date"/>
    	<property name="fetchedFor" column="fetched_for" type="java.sql.Date"/>
    	<property name="language" type="java.lang.String"/>
  </class>
</hibernate-mapping>
